<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="signMapperDao">

	<select id="selectSignInfo" parameterType="HashMap" resultType="HashMap">
		SELECT USER_ID
		     , USER_PW
		     , USER_NAME
		  FROM IRON_DRUM_USER
	</select>

</mapper>